                                            <td><%=orders.paymentMethod %>  </td>
                                            <td>
                                                <% if(orders.orderStatus=='placed') {%>
                                                    <span class="badge rounded-pill alert-success text-success">
                                                        <%=orders.orderStatus %>
                                                    </span>
                                                    <% }else if (orders.orderStatus=='Cancelled' )  { %>
                                                        <span
                                                            class="badge rounded-pill alert-danger text-danger">
                                                            <%= orders.orderStatus %>
                                                        </span>
                                                        <% }else{ %>
                                                            <span
                                                                class="badge rounded-pill alert-warning text-warning">
                                                                <%= orders.orderStatus %>
                                                            </span>
        
                                                        <% } %>
                                            </td>
                                            <td> <a href="/order-detailes/<%=orders._id%>" class="btn btn-dark rounded font-sm mt-15">View
                                                details</a>
                                            </td>
                                        </tr>
                                       
                                        <% }) %>
                                    </tbody>
                                </table>
